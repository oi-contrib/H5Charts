{
  "name": "@vislite/chart",
  "version": "0.1.0",
  "description": "基于VISLite开发的可视化图表库（基础布局版本可跨端，类似无头ECharts）",
  "main": "dist/chart.min.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "rollup --config ./rollup.config.js -w",
    "build": "rollup --config ./rollup.config.js && terser ./dist/chart.js --compress --mangle --output ./dist/chart.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oi-contrib/H5Charts.git",
    "directory": "modules/vislite-plugin-chart"
  },
  "keywords": [
    "chart",
    "vislite",
    "plugin"
  ],
  "author": "zxl20070701",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oi-contrib/H5Charts/issues"
  },
  "homepage": "https://github.com/oi-contrib/H5Charts/blob/master/modules/vislite-plugin-chart/README.md",
  "devDependencies": {
    "@vislite/canvas": "^0.1.0",
    "pageby": "^0.2.0",
    "rollup": "^4.6.1",
    "terser": "^5.17.1"
  },
  "dependencies": {
    "vislite": "^1.2.0"
  }
}
